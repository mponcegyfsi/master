  <DataSets>
    <DataSet Name="Control_Abonos">
      <Query>
        <DataSourceName>JIRA</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FECHA_DESDE">
            <Value>=Parameters!FECHA_DESDE.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FECHA_HASTA">
            <Value>=Parameters!FECHA_HASTA.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
		   H.*,
		   TAR.IDTAREA,
		   TAR.DESCRIPCION AS TAREA,
		   CG.RSOC AS CLIENTE,
		   CG.ID AS IDCLIENTE


	INTO #TMP_HORAS
	FROM JIRASCHEMA.WORKLOG H

	INNER JOIN JIRASCHEMA.JIRAISSUE				  AS T		ON H.ISSUEID					  = T.ID
	INNER JOIN JIRASCHEMA.PROJECT				  AS P		ON T.PROJECT					  = P.ID
	INNER  JOIN jiraschema.AO_013613_WA_VALUE	  AS ta		ON H.ID							  = tA.WORKLOG_ID	AND tA.WORK_ATTRIBUTE_ID = 13
	INNER  JOIN [GYF10\SQL2005].GYFSI.DBO.tarea	  AS tar	ON convert(varchar,ta.value)	  = tar.IDTAREA
	INNER  JOIN JIRASCHEMA.CUSTOMFIELDVALUE		  AS CJ		ON T.ID							  = CJ.ISSUE		AND CJ.CUSTOMFIELD		 = 10222 -- CAMPO CLIENTE
	INNER  JOIN JIRASCHEMA.customfieldoption	  AS CD		ON CJ.CUSTOMFIELD				  = CD.CUSTOMFIELD  AND CJ.STRINGVALUE		 = CD.ID
	INNER  JOIN [GYF10\SQL2005].gyfsi.DBO._CLI	  AS CG		ON CONVERT(INT,CJ.STRINGVALUE)	  = CG.CLIENTE_JIRA
	INNER  JOIN jiraschema.customfieldvalue		  AS CT		ON CT.ISSUE						  = T.ID			AND CT.CUSTOMFIELD = 10307	-- tipo_gestion

	WHERE P.ID IN (10000,10101) AND H.STARTDATE BETWEEN @FECHA_DESDE AND @FECHA_HASTA AND CG.ID &lt;&gt; 999 AND CT.STRINGVALUE NOT IN (10208,11600) -- QUE NO SEA PRESUPUESTO NI NUEVOS PROYECTOS


	SELECT
		   Cliente,
		  -- IDCLIENTE,
		   
		   
		   CASE WHEN IDCLIENTE IN (112,106) -- NBCH Y BTDF - EL ABONO ES DE USO INDISTINTO
		   THEN
		   (
			  SELECT ISNULL(HORAS,0)
			  FROM [GYF10\SQL2005].GYFSI.DBO.PROYECTO 
			  WHERE IDCLIENTE = A.IDCLIENTE AND IDPROYECTO &lt;&gt; 307 AND IDSISTEMAPADRE = 18 AND TIPOCARGAHORA = 1
		   ) 
		   ELSE 0
		   END 
		   AS [Abono Uso Indistinto],

		   SUM(CONVERT(INT,ISNULL(TIMEWORKED,0))/60/60) [Horas Totales Cargadas],
		
		   CASE IDCLIENTE 
				WHEN 121 -- BICA
				THEN 56
				WHEN 31  -- CORRIENTES
				THEN 16
				WHEN 49  -- FORMOSA
				THEN 160
				WHEN 105 -- SAENZ
				THEN 20
				WHEN 120 --CHUBUT
				THEN 24
				WHEN 109 -- MAS VENTAS
				THEN 20
				WHEN 123 -- LA RIOJA
				THEN 20
				WHEN 124 -- TSA
				THEN 20
				WHEN 113 -- BMR
				THEN 300
				WHEN 110 -- BERSA
				THEN 30
			ELSE 0
			END AS [Abono Desarrollo],

		  (
		   SELECT ISNULL(SUM(CONVERT(INT,ISNULL(TIMEWORKED,0))/60/60),0)
		   FROM #TMP_HORAS
		   WHERE IDTAREA IN (74,37,18,57,60,2,38,50,7,4,62,23,49,48,47,45,8,69,61,3) -- Tareas Desarrollo
				 AND IDCLIENTE = A.IDCLIENTE
		   ) AS Desarrollo,

			CASE IDCLIENTE 
				WHEN 120 --CHUBUT
				THEN 8
				WHEN 49  -- FORMOSA
				THEN 40
				WHEN 113 -- BMR
				THEN 70
			ELSE 0
			END AS [Abono Análisis],


		   (
		   SELECT ISNULL(SUM(CONVERT(INT,ISNULL(TIMEWORKED,0))/60/60),0)
		   FROM #TMP_HORAS
		   WHERE IDTAREA IN (13,29,46,68,1,44,34) -- Tareas Análisis
				 AND IDCLIENTE = A.IDCLIENTE
		   ) AS [Analista Funcional],
		   
		   (
		   SELECT ISNULL(SUM(CONVERT(INT,ISNULL(TIMEWORKED,0))/60/60),0)
		   FROM #TMP_HORAS
		   WHERE IDTAREA IN (58,67,53,51) -- Tareas Project Leader
				 AND IDCLIENTE = A.IDCLIENTE
		   ) AS [Projec Leader],
		   (
		   SELECT ISNULL(SUM(CONVERT(INT,ISNULL(TIMEWORKED,0))/60/60),0)
		   FROM #TMP_HORAS
		   WHERE IDTAREA IN (15,54,52) -- Tareas Project Manager
				 AND IDCLIENTE = A.IDCLIENTE
		   ) AS [Projec Manager],
		   (
		   SELECT ISNULL(SUM(CONVERT(INT,ISNULL(TIMEWORKED,0))/60/60),0)
		   FROM #TMP_HORAS
		   WHERE IDTAREA IN (71,41) -- Tareas DBA
				 AND IDCLIENTE = A.IDCLIENTE
		   ) AS DBA,
		   (
		   SELECT ISNULL(SUM(CONVERT(INT,ISNULL(TIMEWORKED,0))/60/60),0)
		   FROM #TMP_HORAS
		   WHERE IDTAREA IN (65,66,56,35) -- Tareas Arquitecto
				 AND IDCLIENTE = A.IDCLIENTE
		   ) AS Arquitecto,
		   (
		   SELECT ISNULL(SUM(CONVERT(INT,ISNULL(TIMEWORKED,0))/60/60),0)
		   FROM #TMP_HORAS
		   WHERE IDTAREA IN (64,63,70,42,43) -- Tareas Analista DevOps
				 AND IDCLIENTE = A.IDCLIENTE
		   ) AS [Analista DevOps],
		   (
		   SELECT ISNULL(SUM(CONVERT(INT,ISNULL(TIMEWORKED,0))/60/60),0)
		   FROM #TMP_HORAS
		   WHERE IDTAREA IN (73) -- Tareas Administración
				 AND IDCLIENTE = A.IDCLIENTE
		   ) AS Administración,
		   (
		   SELECT ISNULL(SUM(CONVERT(INT,ISNULL(TIMEWORKED,0))/60/60),0)
		   FROM #TMP_HORAS
		   WHERE IDTAREA IN (72) -- Tareas RRHH
				 AND IDCLIENTE = A.IDCLIENTE
		   ) AS RRHH,

		   CASE WHEN IDCLIENTE = 112 -- NBCH - EL ABONO ES DE USO INDISTINTO
		   THEN 1
		   ELSE 0
		   END 
		   AS [Incluye todas las tareas]

	FROM #TMP_HORAS A

	GROUP BY CLIENTE,IDCLIENTE

	ORDER BY CLIENTE

	-- Borro la tabla temporal --

	DROP TABLE #TMP_HORAS

</CommandText>
      </Query>
